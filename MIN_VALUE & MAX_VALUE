// Integer.MAX_VALUE
import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
       int[] arr = {1,200,3,4,5,6765};
       int n = arr.length;
       int tmax = Integer.MIN_VALUE;
       for(int i=0;i<n;i++){
           tmax = Math.max(tmax,arr[i]);
       }
       System.out.println(tmax);    // 6765
    }
}
