//Bubble Sort
import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        int[] arr = {12,43,134,1,3,4};
        for(int i=0;i<arr.length;i++){
            for(int j=i+1;j<arr.length;j++){
                if(arr[i]>arr[j]){
                    int temp = arr[j];
                    arr[j] = arr[i];
                    arr[i] = temp;
                }
            }
            System.out.print(arr[i]+ " ");
        }
    }
}

//output:
1 3 4 12 43 134 
