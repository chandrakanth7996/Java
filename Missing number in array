import java.util.*;
class HelloWorld {
    public static void main(String[] args) {
        int[] arr = {1,2,2,4};
        int sum=0;
        int n = arr.length-1;
        for(int i=0;i<arr.length;i++){
            sum+=arr[i];
        }
        System.out.println(sum); // 9
        int res = n*(n+1)/2;
        int diff = sum - res;
        System.out.println(diff); // 3
    }
}
