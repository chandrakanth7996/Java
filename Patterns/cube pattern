
class HelloWorld {
    public static void main(String[] args) {
        int n=3;
        for(int i=0;i<n;i++){
            for(int j=0;j<n;j++){
                System.out.print(" * ");
                if(j!=n-1){
                    System.out.print("_");
                }
            }
            System.out.println();
        }
    }
}

//output:
 * _ * _ * 
 * _ * _ * 
 * _ * _ * 
