class HelloWorld {
    public static void main(String[] args) {
        for(int i=0;i<5;i++){ //row
            for(int j=0;j<10;j++){ //column
                System.out.print("*");
                if(j!=10-1){
                    System.out.print("_");
                }
            }
            System.out.println();
        }
    }
}

//output: 
*_*_*_*_*_*_*_*_*_*
*_*_*_*_*_*_*_*_*_*
*_*_*_*_*_*_*_*_*_*
*_*_*_*_*_*_*_*_*_*
*_*_*_*_*_*_*_*_*_*
