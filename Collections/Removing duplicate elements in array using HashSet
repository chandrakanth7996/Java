// Removing duplicate elements in array using HashSet

import java.util.*;
import java.util.stream.Collectors;
class HelloWorld {
    public static void main(String[] args) {
       int[] arr = {1,2,3,434,548,1,2,3};
       int n = arr.length;
       HashSet<Integer> hs = new HashSet<>();
       for(int i=0;i<n;i++){
           int num = arr[i];
           hs.add(num);
       }
       System.out.println(hs);   // [1, 2, 434, 3, 548]
    }
}

